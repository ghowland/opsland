<div class="flex

  {% if cur_data.justify and cur_data.justify != 'parent' %}
    {{cur_data.justify}}
  {% else %}
    {{cur_style['__style.container.flex_justify']}}
  {% endif %}

  {{cur_style['__style.container.rounded']}}
  {{cur_style['__style.container.padding']}}
">
  <img src="{{cur_data.image}}" alt="{{cur_data.alt}}"
  class="
  {{cur_style['__style.container.width']}}
  {{cur_style['__style.container.width_max']}}
  {{cur_style['__style.container.width_min']}}
  "
>

</div>
