<div class="p-10">

  {% include 'includes/page/breadcrumb.html.j2' %}

  <br><br>

  <h1 class="text-3xl font-bold underline pb-3 text-blue-600">
    Value: {{ result }}
  </h1>

  {% set search_prompt = "Search" %}
  {% include 'includes/generic/generic_input_search_next_table.html.j2' %}

  {{ tables.table_data_dict | safe }}

  <br><br>
  {{ tables.table_data_list | safe }}

  <br><br>
  
</div>  


<div class="size-2/4 ml-80">
  <canvas id="myChart"></canvas>
</div>

<script>
  const data = {
    labels: ["1", "2", "3", "4", "5", "6", "7", "8", ],
    datasets: [{
      label: 'Example 0 Data',
      data: [65, 59, 80, 81, 56, 55, 40],
      fill: false,
      borderColor: 'rgb(23, 3, 252)',
      tension: 0.1
    }]
  };

  const config = {
    type: 'line',
    data: data,
  };

  const ctx = document.getElementById('myChart');

  new Chart(ctx, config);

</script>


